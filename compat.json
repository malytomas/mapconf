{
  "compatibility-mode": true,
  "constants": {
    "@main-font": [ "noto-mix", "noto-cjk" ],
    "@devil": [ "smiley", 54, 49, 30, 30 ],
    "@sleepy": [ "smiley", 154, 94, 30, 30 ]
  },
  "fonts": {
    "noto-mix": "//cdn.melown.com/libs/vtsjs/fonts/noto-extended/1.0.0/noto.fnt",
    "noto-cjk": "//cdn.melown.com/libs/vtsjs/fonts/noto-cjk/1.0.0/noto.fnt",
    "#default": "//cdn.melown.com/libs/vtsjs/fonts/noto-basic/1.0.0/noto.fnt"
  },
  "bitmaps": {
    "smiley": "smiley.jpeg"
  },
  "layers": {
    "icons-flat-ratio": {
      "filter": [
        "all",
        [ "==", "#group", "place" ],
        [ "==", "$class", "town" ],
        [
          "==",
          {
            "mod": [
              { "strlen": "$name" },
              4
            ]
          },
          0
        ]
      ],
      "point": true,
      "point-flat": true,
      "point-radius": 0.02,
      "point-radius-units": "ratio",
      "point-color": [ 255, 0, 0, 120 ],
      "zbuffer-offset": [ -0.2, 0, 0 ],
      "z-index": 2,
      "culling": 89
    },
    "icons-screen-ratio": {
      "filter": [
        "all",
        [ "==", "#group", "place" ],
        [ "==", "$class", "town" ],
        [
          "==",
          {
            "mod": [
              { "strlen": "$name" },
              4
            ]
          },
          1
        ]
      ],
      "point": true,
      "point-flat": false,
      "point-radius": 0.02,
      "point-radius-units": "ratio",
      "point-color": [ 0, 0, 255, 120 ],
      "zbuffer-offset": [ -0.2, 0, 0 ],
      "z-index": 4,
      "culling": 89
    },
    "icons-flat": {
      "filter": [
        "all",
        [ "==", "#group", "place" ],
        [ "==", "$class", "town" ],
        [
          "==",
          {
            "mod": [
              { "strlen": "$name" },
              4
            ]
          },
          2
        ]
      ],
      "point": true,
      "point-flat": true,
      "point-radius": 2000,
      "point-color": [ 255, 255, 0, 120 ],
      "zbuffer-offset": [ -0.2, 0, 0 ],
      "z-index": 2,
      "culling": 89
    },
    "icons-screen": {
      "filter": [
        "all",
        [ "==", "#group", "place" ],
        [ "==", "$class", "town" ],
        [
          "==",
          {
            "mod": [
              { "strlen": "$name" },
              4
            ]
          },
          3
        ]
      ],
      "point": true,
      "point-flat": false,
      "point-radius": 40,
      "point-color": [ 0, 255, 255, 120 ],
      "zbuffer-offset": [ -0.2, 0, 0 ],
      "z-index": 4,
      "culling": 89
    }
  }
}
