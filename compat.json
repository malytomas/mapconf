{
  "compatibility-mode": true,
  "constants": {
    "@main-font": [ "noto-mix", "noto-cjk" ],
    "@devil": [ "smiley", 54, 49, 30, 30 ],
    "@sleepy": [ "smiley", 154, 94, 30, 30 ]
  },
  "fonts": {
    "noto-mix": "//cdn.melown.com/libs/vtsjs/fonts/noto-extended/1.0.0/noto.fnt",
    "noto-cjk": "//cdn.melown.com/libs/vtsjs/fonts/noto-cjk/1.0.0/noto.fnt",
    "#default": "//cdn.melown.com/libs/vtsjs/fonts/noto-basic/1.0.0/noto.fnt"
  },
  "bitmaps": {
    "smiley": "smiley.jpeg"
  },
  "layers": {
    "lines": {
      "filter": [ "==", "#group", "transportation" ],
      "line": true,
      "line-flat": false,
      "line-width": 0.01,
      "line-width-units": "ratio",
      "line-color": [ 255, 0, 0, 120 ],
      "zbuffer-offset": [ -0.2, 0, 0 ],
      "z-index": 1,
      "culling": 89
    }
  }
}
