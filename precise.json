{
  "compatibility-mode": false,
  "constants": {
    "@main-font": [ "noto-mix", "noto-cjk" ],
    "@smileys": [
      [
        0,
        [ "smiley", 54, 49, 30, 30 ]
      ],
      [
        1,
        [ "smiley", 154, 94, 30, 30 ]
      ],
      [
        2,
        [ "smiley", 7, 5, 27, 27 ]
      ],
      [
        3,
        [ "smiley", 7, 51, 27, 27 ]
      ],
      [
        4,
        [ "smiley", 56, 97, 27, 27 ]
      ],
      [
        5,
        [ "smiley", 106, 143, 26, 27 ]
      ]
    ],
    "@smiley": {
      "map": [
        {
          "mod": [
            { "strlen": "$name" },
            6
          ]
        },
        "@smileys",
        "blah"
      ]
    }
  },
  "fonts": {
    "noto-mix": "//cdn.melown.com/libs/vtsjs/fonts/noto-extended/1.0.0/noto.fnt",
    "noto-cjk": "//cdn.melown.com/libs/vtsjs/fonts/noto-cjk/1.0.0/noto.fnt",
    "#default": "//cdn.melown.com/libs/vtsjs/fonts/noto-basic/1.0.0/noto.fnt"
  },
  "bitmaps": {
    "smiley": "smiley.png"
  },
  "layers": {
    "places": {
      "filter": [
        "all",
        [ "==", "#group", "place" ]
      ],
      "pack": true,
      "label": true,
      "label-stick": [ 50, 0, 10, 255, 255, 255, 128 ],
      "label-size": 15.2,
      "label-font": "@main-font",
      "label-source": "$name",
      "label-offset": [ -20, 0 ],
      "label-origin": "bottom-left",
      "label-no-overlap-margin": [ 10, 10 ],
      "label-no-overlap": true,
      "icon": true,
      "icon-source": "@smiley",
      "icon-origin": "bottom-right",
      "icon-offset": [ -25, 0 ],
      "icon-no-overlap-margin": [ 10, 10 ],
      "icon-no-overlap": true,
      "icon-scale": 1,
      "zbuffer-offset": [ -0.15, 0, 0 ],
      "culling": 89
    }
  }
}
